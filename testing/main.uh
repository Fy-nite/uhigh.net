namespace command {
    type CommandList = array<Command>
    
    class Command {
        field name: string
        field description: string
        field usage: string
        field aliases: array<string>
        field options: array<string>
        field arguments: array<string>
        field subcommands: CommandList
    }
    
    class Program {
        public static func Main(args: array<string>): void {
            // Example usage of Command class
            Console.WriteLine("Command Example")
            var m = Console.ReadLine()
            match m {
                "help" => {
                    Console.WriteLine("Showing help...")
                    Console.WriteLine("Help displayed")
                },
                "exit" => {
                    Console.WriteLine("Exiting program")
                },
                _ => {
                    Console.WriteLine("Unknown command: " + m)
                    Console.WriteLine("Error")
                }
            }
        }
    }
}
