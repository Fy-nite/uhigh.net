{
  "name": "uhigh-syntax",
  "displayName": "μHigh Language Support",
  "description": "Syntax highlighting for μHigh programming language",
  "version": "1.0.0",
  "publisher": "uhigh-lang",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "uhigh",
    "μhigh",
    "programming",
    "language"
  ],
  "contributes": {
    "languages": [
      {
        "id": "uhigh",
        "aliases": [
          "μHigh",
          "uhigh",
          "UHigh"
        ],
        "extensions": [
          ".uh"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/uhigh-light.png",
          "dark": "./icons/uhigh-dark.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "uhigh",
        "scopeName": "source.uhigh",
        "path": "./uhigh.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/uhigh.net"
  },
  "license": "MIT"
}
