using System
using StdLib
namespace test
{
    public class Program
    {
        public static field x: Observable<int> = new Observable<int>(0, true)
        public static func Main(args: string[]): void
        {
            x.Subscribe((value) => {
                Console.WriteLine("Value changed to: " + value);
            });
            x.Value = 10;
            x.Value = 20;
            x.Value = 30;
        }
    }
}